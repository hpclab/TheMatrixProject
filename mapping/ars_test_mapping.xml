	<!-- mapping of DRUG -->
	<dataset name="DRUG">
		<joinName>DRUG_JOIN</joinName>
		<joinClause>FLUSSI.U_SPF1_EXT JOIN FLUSSI.U_SPF2_EXT ON FLUSSI.U_SPF1_EXT.ID_SPF1_ARSNEW=FLUSSI.U_SPF2_EXT.ID_SPF1_ARSNEW</joinClause>
