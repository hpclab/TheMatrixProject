		<simpleMapping name="SECONDARY_PROC_4">
			<sourceTable>FLUSSI.U_SKNO_EXT</sourceTable>
			<sourceAttribute>CODCHI5</sourceAttribute>
		</simpleMapping>
		<simpleMapping name="SECONDARY_PROC_5">
